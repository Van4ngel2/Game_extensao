CREATE TABLE usuario (
    id INT AUTO_INCREMENT PRIMARY KEY,
    username VARCHAR(255) UNIQUE,
    senha VARCHAR(255) -- Use uma t√©cnica segura para armazenar senhas, como hashing
);

CREATE TABLE score (
    id INT AUTO_INCREMENT PRIMARY KEY,
    usuario_id INT,
    score INT,
    FOREIGN KEY (usuario_id) REFERENCES usuario(id)
);
